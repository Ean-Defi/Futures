<script type="text/javascript">
    var ser = 0.005;
    $(document).ready(function () {
        var laydate = layui.laydate;
        laydate.render({
            elem: '#startTime',
            type: 'datetime',
            format: 'yyyy-MM-dd HH:mm:ss'
        });
        laydate.render({
            elem: '#endTime',
            type: 'datetime',
            format: 'yyyy-MM-dd HH:mm:ss'
        });
        laydate.render({
            elem: '#dual-startTime',
            type: 'datetime',
            format: 'yyyy-MM-dd HH:mm:ss'
        });
        laydate.render({
            elem: '#dual-endTime',
            type: 'datetime',
            format: 'yyyy-MM-dd HH:mm:ss'
        });

        if (sessionStorage.getItem('style')) {
            if (sessionStorage.getItem('style') == 'Light') {
                $(document.body).addClass('Light');
                bgColor = '#fafafa';
                hColor = '#eee';
                vColor = '#eee';
                color = '#666';
                upColor = '#01B1A7';
                downColor = '#f75467';
            } else {
                $(document.body).addClass('dark');
                bgColor = '#030D2D';
                hColor = 'rgba(41, 46, 79, 0.1)';
                vColor = 'rgba(41, 46, 79, 0.1)';
                color = '#959EC6';
                upColor = '#01b1a7';
                downColor = '#f75467';
            }
        } else {
            $(document.body).addClass('dark');
            bgColor = '#030D2D';
            hColor = '#222';
            vColor = '#222';
            color = '#959EC6';
            upColor = '#01b1a7';
            downColor = '#f75467';
            sessionStorage.setItem('style', 'dark');
        }
        var daytime = document.getElementById('dark'),
            night = document.getElementById('default');
        daytime.addEventListener('click', function () {
            $(document.body).removeClass('Light').addClass('dark');
            sessionStorage.setItem('style', 'dark');
            $('#dark').hide();
            $('#default').show();
            bgColor = '#030D2D';
            hColor = 'rgba(41, 46, 79, 0.1)';
            vColor = 'rgba(41, 46, 79, 0.1)';
            color = '#959EC6';
            upColor = '#01b1a7';
            downColor = '#f75467';
            $('iframe')[0].contentWindow.init();
            widget.applyOverrides({
                "paneProperties.background": bgColor,
                "paneProperties.horzGridProperties.color": hColor,
                "paneProperties.vertGridProperties.color": vColor,
                "scalesProperties.textColor": color,
                "mainSeriesProperties.candleStyle.upColor": upColor,
                "mainSeriesProperties.candleStyle.downColor": downColor,
            });
            widget.applyStudiesOverrides({
                "volume.volume.color.0": upColor,
                "volume.volume.color.1": downColor,
            });
        });

        night.addEventListener('click', function () {
            $(document.body).removeClass('dark').addClass('Light');
            sessionStorage.setItem('style', 'Light');
            $('#default').hide();
            $('#dark').show();
            bgColor = '#fafafa';
            hColor = '#eee';
            vColor = '#eee';
            color = '#666';
            upColor = '#01B1A7';
            downColor = '#f75467';
            $('iframe')[0].contentWindow.init();
            widget.applyOverrides({
                "paneProperties.background": bgColor,
                "paneProperties.horzGridProperties.color": hColor,
                "paneProperties.vertGridProperties.color": vColor,
                "scalesProperties.textColor": color,
                "mainSeriesProperties.candleStyle.upColor": upColor,
                "mainSeriesProperties.candleStyle.downColor": downColor,
            });
            widget.applyStudiesOverrides({
                "volume.volume.color.0": upColor,
                "volume.volume.color.1": downColor
            });
        });
        resizeScreen();
        drapScreen();//拖动改变窗口高度
    });
    var bgColor = '#030D2D',
        hColor = 'rgba(41, 46, 79, 0.1)',
        vColor = 'rgba(41, 46, 79, 0.1)',
        color = '#eee',
        upColor = '#01B1A7',
        downColor = '#f75467';
    $(function () {
        var url_fu_name = getParameterByName('fu_name');
        if (url_fu_name !== '') {
            $('#sel_futures').val(url_fu_name); // 选中合约币种
        }
        //
        var wsUrl = "wss://www.eanex.io/wsfuture";
        var fu_name = $('#sel_futures').val();
        var future_name = "BTC_USDT";
        // 切换合约币种
        $('#buyAdd').val($('#sel_futures option:selected').attr('data-add'));
        $('#sel_futures').on('change', function(){
            $('#buyAdd').val($('#sel_futures option:selected').attr('data-add'));
            window.location.href = WST.U('future/index/index') + '?fu_name=' + $('#sel_futures').val();

        });
        $('#fu_name').val(fu_name);
        var price = Number($('#latestPrice').text());
        var buyAdd = Number($('#buyAdd').val());
        $('.buyPrice').text((price + buyAdd).toFixed(2));
        $('.sellPrice').text((price - buyAdd).toFixed(2));
        //初始化该交易市场的推送
        socketPush(fu_name, wsUrl);
        //K线图点击
        // changePeroid(fu_name,timeType);
        initPrice(future_name); // 初始化行情报价数据
        initFutureLever(fu_name); // 初始化合约杠杆数据
        $('.Varieties').text($("#sel_futures  option:selected").text());
        $('.latestPrice').text("35281.71");
        holeFn();  //当前持仓
        entrustFn(); //挂单委托
        dealFn();//历史成交
        historyRegisterFn();//历史挂单
        assetsFn();//资产管理


    });
    function drapScreen(){
        var src_posi_Y = 0, dest_posi_Y = 0, move_Y = 0, is_mouse_down = false, destHeight = 200;
        $("#drap").mousedown(function(e){
            src_posi_Y = e.pageY;
            is_mouse_down = true;
        });
        $(document).bind("click mouseup",function(e){
            if(is_mouse_down){
                is_mouse_down = false;
            }
        })
            .mousemove(function(e){
                dest_posi_Y = e.pageY;
                move_Y = src_posi_Y - dest_posi_Y;
                src_posi_Y = dest_posi_Y;
                destHeight = $("#panel-tab").height() + move_Y;
                if(is_mouse_down && destHeight<360){
                    resizeScreen();
                    //判断destHeight是否大于200
                    if(document.body.clientWidth>1366){
                        $(".panel-bottom").css("height", destHeight > 230 ? destHeight : 230);
                    }else{
                        $(".panel-bottom").css("height", destHeight > 200 ? destHeight : 200);
                    }

                }
            });
    }
    function resizeScreen() {
        var tradeCont = document.getElementById('trade-cont');
        var chart = document.getElementById('chart');
        var marketCont = document.getElementById('marketCont');
        var contractCont = document.getElementById('contractCont');
        var limitCont = document.getElementById('limitCont');
        var propertyCont = document.getElementById('propertyCont');
        var contractTab = document.getElementById('contractTab');
        var marketTable = document.getElementById('marketTable');
        var propertyTable = document.getElementById('propertyTable');
        var limitTab = document.getElementById('limitTab');
        tradeCont.style.height = $('body').height() - $('.header-top').height() - $('.panel-bottom').height()-10 + 'px';
        chart.style.height = $('body').height() - $('.header-top').height() - $('.panel-bottom').height() - $('.chart-header').height()-20+ 'px';
        marketCont.style.height = ($('body').height() - $('.header-top').height() - $('.panel-bottom').height()) / 2 - 10 + 'px';
        contractCont.style.height = ($('body').height() - $('.header-top').height() - $('.panel-bottom').height()) / 2 - 10 + 'px';
        limitCont.style.height = ($('body').height() - $('.header-top').height() - $('.panel-bottom').height()) / 2 + 11 + 'px';
        propertyCont.style.height = ($('body').height() - $('.header-top').height() - $('.panel-bottom').height()) / 2 + 11 + 'px';

        contractTab.style.height = ($('body').height() - $('.header-top').height() - $('.panel-bottom').height()) / 2 - 110 + 'px';
        marketTable.style.height = ($('body').height() - $('.header-top').height() - $('.panel-bottom').height()) / 2 - 110 + 'px';
        propertyTable.style.height = ($('body').height() - $('.header-top').height() - $('.panel-bottom').height()) / 2 - 81 + 'px';
        limitTab.style.height = ($('body').height() - $('.header-top').height() - $('.panel-bottom').height()) / 2 - 80 + 'px';

        var windowWidth = $(window).width();
        var clientW = document.body.clientWidth/2;
        if(windowWidth<1479) {
            $('.trade-wrapper .trade-panel').css({'width':clientW+60+'px'});
            $('.trade-wrapper .trade-book').css({'width':clientW-60+'px',left:clientW+60+'px'});
        }else {
            $('.trade-wrapper .trade-panel').css({'width':'58%'});
            $('.trade-wrapper .trade-book').css({'width':'42%',left:'58%'});
        }
    }

    window.addEventListener("resize", resizeScreen);

</script>
</body>
</html>
